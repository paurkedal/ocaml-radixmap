(jbuild_version 1)

(library
 ((name radixmap)
  (public_name radixmap)
  (wrapped false)
  (modules
   (Bitword
    Bitword_radixmap
    Bitword_radixmap_sig))))

(library
 ((name radixmap_ipaddr)
  (public_name radixmap-ipaddr)
  (wrapped false)
  (modules
   (Ip_radixset
    Ip_radixset_sig
    Ip_radixmap
    Ip_radixmap_sig))
  (libraries (ipaddr radixmap))))

(rule
 ((targets (bitword_radixmap_sig.ml))
  (deps (bitword_radixmap_sig.mli))
  (action (copy ${<} ${@}))))

(rule
 ((targets (ip_radixmap_sig.ml))
  (deps (ip_radixmap_sig.mli))
  (action (copy ${<} ${@}))))

(rule
 ((targets (ip_radixset_sig.ml))
  (deps (ip_radixset_sig.mli))
  (action (copy ${<} ${@}))))
